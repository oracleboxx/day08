class Member01{
	/*
	 * 1. class 키워드로 클래스명 Member01을 정의
	 * 2. 클래스명 첫글자는 관례적으로 영문대문자로 한다.
	 * 3. 
	 */
	String name;
	int age;
	
}
public class Test02 {

	public static void main(String[] args) {
		Member01 men = new Member01();
		men.name = "홍길동";
		men.age = 26;
		System.out.println("이름:"+men.name);
		System.out.println("이름:"+men.age);
		
	}

}
